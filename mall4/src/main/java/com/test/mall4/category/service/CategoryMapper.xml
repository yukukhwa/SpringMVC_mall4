<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.test.mall4.category.service.CategoryMapper">
 	<select id="selectCategoryList"
 		resultType="com.test.mall4.category.service.Category">
 		SELECT 
 			category_no as categoryNo
 			,category_name as categoryName
 		FROM category
 	</select>
 	<insert id="insertCategory" parameterType="com.test.mall4.category.service.Category">
 		INSERT INTO category
		(category_name)
		VALUES (#{categoryName})
 	</insert>
</mapper>

